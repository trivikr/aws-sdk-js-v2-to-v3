{
  "name": "aws-sdk-js-v2-to-v3",
  "version": "0.1.0",
  "description": "jscodeshift transform to migrate AWS SDK for JavaScript in a codebase from v2 to v3.",
  "keywords": [
    "jscodeshift",
    "transform",
    "aws",
    "sdk",
    "javascript",
    "v2",
    "v3"
  ],
  "homepage": "https://github.com/trivikr/aws-sdk-js-v2-to-v3",
  "license": "MIT",
  "author": "Kamat, Trivikram <trivikr.dev@gmail.com>",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/trivikr/aws-sdk-js-v2-to-v3.git"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint 'src/*.ts'",
    "release": "yarn build && changeset publish",
    "test": "jest",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "jscodeshift": "0.13.1",
    "jscodeshift-find-imports": "2.0.4"
  },
  "devDependencies": {
    "@changesets/cli": "2.21.0",
    "@tsconfig/node12": "1.0.2",
    "@types/jest": "27.4.1",
    "@types/jscodeshift": "0.11.3",
    "@types/node": "16.11.26",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "eslint": "8.10.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "jest": "27.5.1",
    "lint-staged": "12.3.4",
    "prettier": "2.5.1",
    "simple-git-hooks": "2.7.0",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "packageManager": "yarn@3.2.0",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  }
}
